                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/txt_margin_top"
                        android:gravity="center"
                        android:text="@string/relation"
                        android:textColor="@color/bg_white"
                        android:textSize="@dimen/txt_main_size"
                        android:textStyle="bold" />
                </LinearLayout>

                <View
                    android:id="@+id/ll_relation_view"
                    android:layout_width="@dimen/view_width"
                    android:layout_height="@dimen/view_height"
                    android:background="@color/bg_login"
                    android:visibility="visible">

                </View>

                <LinearLayout
                    android:id="@+id/ll_sign"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/linear_height"
                    android:layout_weight="1"
                    android:background="@color/bg_menu"
                    android:contentDescription="@string/sign"
                    android:gravity="center"
                    android:orientation="vertical">
                    <!--android:layout_height="@dimen/main_height"-->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/del_margin_top"
                        android:layout_marginBottom="@dimen/del_margin_bottom"
                        android:gravity="center"
                        android:textColor="@color/bg_white"
                        android:textSize="@dimen/txt_main_size"
                        android:textStyle="bold"
                        android:visibility="invisible" />

                    <ImageView
                        android:id="@+id/image_sign"
                        android:layout_width="@dimen/image_width"
                        android:layout_height="@dimen/image_height"
                        android:layout_marginLeft="@dimen/margin_left"
                        android:src="@drawable/camera" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/txt_margin_top"
                        android:gravity="center"
                        android:text="@string/sign"
                        android:textColor="@color/bg_white"
                        android:textSize="@dimen/txt_main_size"
                        android:textStyle="bold" />
                </LinearLayout>

            </LinearLayout>


            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="@dimen/lay_padding"
                android:paddingLeft="@dimen/lay_padding_left"
                android:paddingRight="@dimen/lay_padding_right"
                android:visibility="visible"
                android:weightSum="2">

                <LinearLayout
                    android:id="@+id/ll_complete"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/linear_height"
                    android:layout_weight="1"
                    android:background="@color/bg_menu"
                    android:contentDescription="@string/complete"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:scaleType="fitCenter"
                    android:visibility="invisible">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/del_margin_top"
                        android:layout_marginBottom="@dimen/del_margin_bottom"
                        android:gravity="center"
                        android:textColor="@color/bg_white"
                        android:textSize="@dimen/txt_main_size"
                        android:textStyle="bold"
                        android:visibility="invisible" />

                    <ImageView
                        android:id="@+id/iv_complete"
                        android:layout_width="@dimen/image_width"
                        android:layout_height="@dimen/image_height"
                        android:layout_marginLeft="@dimen/margin_left"
                        android:src="@drawable/checkmark" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/txt_margin_top"
                        android:gravity="center"
                        android:text="@string/complete"
                        android:textColor="@color/bg_white"
                        android:textSize="@dimen/txt_main_size"
                        android:textStyle="bold" />
                </LinearLayout>
                <!-- Complete press Ends-->


            </LinearLayout>

