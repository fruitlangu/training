                <LinearLayout
                    android:id="@+id/ll_imag_invoice"
                    android:layout_width="@dimen/main_width"
                    android:layout_height="@dimen/linear_height"
                    android:layout_weight="1"
                    android:background="@color/bg_menu"
                    android:contentDescription="@string/invoice"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:scaleType="centerCrop">
                    <!--android:layout_height="@dimen/main_height"-->

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:gravity="center"
                        android:textColor="@color/bg_white"

                        android:textSize="@dimen/txt_main_size"
